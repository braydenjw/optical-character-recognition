<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.willenborg.annocr.gui.controllers.MainController">
   <children>
      <VBox>
         <children>
            <Button fx:id="nextButton" mnemonicParsing="false" onAction="#nextButtonPressed" text="Next Step" textAlignment="CENTER" />
            <ImageView fx:id="previewImage" pickOnBounds="true" preserveRatio="true" />
         </children>
      </VBox>
   </children>
</Pane>
